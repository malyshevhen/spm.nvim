{
  "diagnostics.globals": [
    "vim",
    "describe",
    "it",
    "before_each",
    "after_each",
    "assert",
    "pending",
    "setup",
    "teardown",
    "clear"
  ],
  "workspace.library": [
    "./test/plenary.nvim/lua",
    "./test/plenary.nvim/lua/plenary/_meta",
    "${3rd}/luv/library"
  ],
  "workspace.checkThirdParty": false,
  "runtime.path": [
    "lua/?.lua",
    "lua/?/init.lua",
    "./test/plenary.nvim/lua/?.lua",
    "./test/plenary.nvim/lua/?/init.lua",
    "../plenary.nvim/lua/?.lua",
    "../plenary.nvim/lua/?/init.lua",
    "../../test/plenary.nvim/lua/?.lua",
    "../../test/plenary.nvim/lua/?/init.lua"
  ],
  "completion.enable": true,
  "completion.keywordSnippet": "Replace",
  "completion.callSnippet": "Replace",
  "workspace.ignoreDir": [".git"]
}
