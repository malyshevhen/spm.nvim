{
  "diagnostics.globals": [
    "vim",
    "describe",
    "it",
    "before_each",
    "after_each",
    "assert",
    "pending",
    "setup",
    "teardown",
    "clear"
  ],
  "workspace.library": [
    "${3rd}/luv/library",
    "${3rd}/busted/library",
    "${3rd}/luassert/library"
  ],
  "workspace.checkThirdParty": false,
  "runtime.path": ["lua/?.lua", "lua/?/init.lua"],
  "completion.enable": true,
  "completion.keywordSnippet": "Replace",
  "completion.callSnippet": "Replace",
  "workspace.ignoreDir": [".git"]
}
